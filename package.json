{
  "name": "vrb-shop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://vigolajnen.github.io/projects/vrb-shop/",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "dev": "cross-env REACT_APP_VERSION=dev vite",
    "dev:build": "tsc && vite build --mode development",
    "prod": "cross-env REACT_APP_VERSION=prod vite --mode production",
    "prod:build": "cross-env REACT_APP_VERSION=prod tsc && vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "framer-motion": "^10.16.16",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-helmet-async": "^2.0.4",
    "react-hook-form": "^7.49.2",
    "react-input-mask": "^2.0.4",
    "react-redux": "^9.0.4",
    "react-router": "^6.21.1",
    "react-router-dom": "^6.21.1",
    "react-tooltip": "^5.25.0",
    "swiper": "^11.0.5",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-helmet-async": "^1.0.3",
    "@types/react-input-mask": "^3.0.5",
    "@types/react-redux": "^7.1.33",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.1",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.0.2",
    "vite": "^5.0.8",
    "vite-imagetools": "^6.2.8",
    "vite-plugin-env-compatible": "^2.0.1",
    "vite-plugin-svgr": "^4.2.0"
